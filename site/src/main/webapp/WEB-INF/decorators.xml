<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
    <decorator name="basic-theme" page="basic-theme.jsp">
        <pattern>/data/*</pattern>
    </decorator>
    
    <decorator name="admin-theme" page="admin-theme2.jsp">
        <pattern>/admin/*</pattern>
    </decorator>
    
    <decorator name="all-theme" page="basic-theme.jsp">
        <pattern>/*</pattern>
    </decorator>
    
    <excludes>
    <pattern>*/ajax/*</pattern>
    <pattern>/data/plaintext/*</pattern>
    <pattern>/index.jsp</pattern>
    <pattern>/userList_Json.jsp</pattern>
  </excludes>
  
</decorators>